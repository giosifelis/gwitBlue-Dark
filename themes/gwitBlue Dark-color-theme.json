{
  "name": "gwitBlue Dark",
  "colors": {
    "editor.background": "#171819",
    "editorCursor.foreground": "#FF0000",
    "editor.foreground": "#F4F4F4",
    "editorWhitespace.foreground": "#3B3A32",
    "editor.lineHighlightBackground": "#3E3D32",
    "editor.selectionBackground": "#49483E"
  },
  "tokenColors": [
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#5F5F5F",
        "fontStyle": " italic"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "string",
        "invalid.illegal.placeholder",
        "constant.other.placeholder"
      ],
      "settings": {
        // "foreground": "#e4c674"
        "foreground": "#BFD985"
      }
    },
    {
      "scope": ["constant.numeric", "constant.character.escape"],
      "settings": {
        "foreground": "#fda86e"
      }
    },
    {
      "scope": [
        "keyword.control",
        "storage.modifier",
        "storage.type",
        "keyword.operator.new",
        "variable.language.this.cpp",
        "punctuation.definition.directive.c",
        "constant.language",
        "support.type.sys-types",
        "keyword.operator.sizeof"
      ],
      "settings": {
        "foreground": "#C4605A",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#BFD985",
        "fontStyle": ""
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#8AB3D2"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#8AB3D2"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#8AB3D2"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#8AB3D2"
      }
    },
    {
      "name": "css attribute name",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#8AB3D2"
      }
    },
    {
      "name": "html punctuations",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#8AB3D2"
      }
    },
    {
      "name": "html attribute name",
      "scope": [
        "entity.other.attribute-name",
        "punctuation.definition.string.html"
      ],
      "settings": {
        "foreground": "#B2AFAF"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "keyword.other.unit.css"],
      "settings": {
        "foreground": "#BFD985"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character",
      "settings": {
        "foreground": "#BFD985"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#BFD985"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#BFD985"
      }
    },
    {
      "name": "JSON String",
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#f4f4f4"
      }
    },
    // {
    //   "name": "Support Variable",
    //   "scope": ["support.variable", "support.constant"],
    //   "settings": {
    //     // "foreground": "#f4f4f4"
    //     "foreground": "#8AB3D2"
    //   }
    // },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#F4F4F4"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        // "foreground": "#c3daec"
        "foreground": "#bbbbbb"
      }
    }
  ],
  "colorSpaceName": "sRGB"
}
